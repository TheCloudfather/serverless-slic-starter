{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "format": "eslint --cache --fix \"**/*.js\"",
    "fetch-deployment-config": "node scripts/fetch-deployment-config",
    "start": "SLIC_STAGE=local react-scripts start",
    "start-any": "react-scripts start",
    "react-app-build": "node node_modules/react-scripts/bin/react-scripts build",
    "build": "npm run fetch-deployment-config && npm run react-app-build",
    "test": "npm run lint",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@aws-amplify/api": "^4.0.21",
    "@aws-amplify/auth": "^4.3.11",
    "@aws-amplify/core": "^4.3.3",
    "@clampy-js/clampy": "github:clampy-js/clampy",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.5.1",
    "awscred": "^1.5.0",
    "buffer": "^6.0.3",
    "clamp-js": "^0.7.0",
    "connected-react-router": "^6.6.1",
    "date-fns": "^2.24.0",
    "http-proxy-middleware": "^0.19.1",
    "line-clamp": "1.0.0",
    "mock-amplify-auth": "^2.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "4.0.3",
    "react-text-truncate": "^0.14.0",
    "react-truncate-markup": "^3.0.1",
    "redux": "^4.0.5",
    "redux-api-middleware": "^3.2.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "serverless-s3-sync": "^1.17.1",
    "string-to-color": "^2.0.1",
    "typescript": "^3.7.5",
    "yaml": "^1.7.2"
  }
}
